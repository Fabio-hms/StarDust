Identificadores = {[a-z_][a-zA-Z0-9_]*}
Literais :
Numero Inteiro = {[+-]?\d+}
Numero Decimal = {[+-]?(\d+\.\*|\.\d+)([eE][+-]?\d+)?}
String = {'([^'\\]|zz.)*'}
String Double = {"([^"\\]|zz.)*"}

Operadores:
Operador de Igualdade : "=="
Operador de Diferente de : "!="
Operador de Maior ou igual que : ">="
Operador de Menor ou igual que : "<="
Operador de Maior que: ">"
Operador de Menor que: "<"
Operador de Atribuição : "="
Operador de Adição : "+"
Operador de Subtração : "-"
Operador de Multiplicação : "*"
Operador de Divisão : "/"
Operador de Números inteiros da Divisão : "//"
Operador de Resto da Divisão : "%"

Delimitadores:
Delimitador InícioParenteses : "("
Delimitador FimParenteses : ")"
Delimitador InícioChaves : "{"
Delimitador FimChaves : "}"
Delimitador Linha : ";"

Comentarios:

Comentario Linha : "\\"
Comentario InicioBloco : "/*"
Comentario FimBLoco : "*/"

Palavras-chave:
{if, else, elsif, for, while, return, def, true, false, and,or,null,function}
